{# @ var collector \Sidus\EAVModelBundle\Profiler\QueryBuilderCollector #}
{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        {{ include('@SidusEAVModel/Profiler/icon.svg') }}
        <span class="sf-toolbar-value">{{ collector.joins|length }} EAV Queries</span>
    {% endset %}

    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>EAV Queries</b>
            <span class="sf-toolbar-status">{{ collector.joins|length }}</span>
        </div>
    {% endset %}

    {{ include('@WebProfiler/Profiler/toolbar_item.html.twig', { 'link': true }) }}
{% endblock %}

{% block menu %}
    <span class="label">
        <span class="icon">{{ include('@SidusEAVModel/Profiler/icon.svg') }}</span>
        <strong>EAV Queries</strong>
    </span>
{% endblock %}

{% block panel %}
    <h2>EAV Queries</h2>
    {{ dump(collector.queryBuilders) }}
    {{ dump(collector.joins) }}
{% endblock %}
